
tip-integrate auto-sched-next		|| { echo error1; exit -1; }
tip-integrate auto-x86-next		|| { echo error2; exit -1; }
tip-integrate auto-latest		|| { echo error3; exit -1; }

tip-checkout auto-latest		|| { echo error4; exit -1; }
D=`date | sed 's/[ :]/_/g'`

cat .git/refs/heads/auto-latest > .git/refs/heads/master ||
					   { echo error6; exit -1; }

tip-checkout master

# save the new master head:
git-tag -m "auto-latest update: $D" tip-auto-history-$D master ||
					   { echo error5; exit -1; }

#
# Do some backups ...
#
date				>> ~/tip-master.log
cat .git/refs/heads/master	>> ~/tip-master.log
echo				>> ~/tip-master.log

